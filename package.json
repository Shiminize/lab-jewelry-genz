{
  "name": "glowglitch-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node --expose-gc server.js",
    "dev:next": "next dev",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "lint": "next lint",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:compliance": "./scripts/run-compliance-tests.sh",
    "test:api": "playwright test tests/api-compliance.spec.ts",
    "test:3d": "playwright test tests/3d-customizer-acceptance.spec.ts",
    "test:a11y": "playwright test tests/accessibility-compliance.spec.ts",
    "test:perf": "playwright test tests/performance-compliance.spec.ts",
    "test:design": "playwright test tests/design-system-compliance.spec.ts",
    "test:arch": "playwright test tests/component-architecture-compliance.spec.ts",
    "test:url": "node scripts/test-url-parameters.js",
    "test:url:mobile": "node scripts/test-url-parameters.js --mobile",
    "test:url:headed": "node scripts/test-url-parameters.js --headed",
    "test:url:debug": "node scripts/test-url-parameters.js --debug",
    "test:quality": "node scripts/test-image-quality.js",
    "generate:3d": "node scripts/generate-3d-sequences.js",
    "batch-generate": "node scripts/batch-sequence-generator.js",
    "optimize-sequences": "node scripts/optimize-sequences.js",
    "generate-quick-fix": "node scripts/batch-sequence-generator.js quick-fix",
    "qa": "node scripts/qa-check.js",
    "qa:quick": "node scripts/qa-check.js --quick",
    "qa:full": "node scripts/qa-check.js --full",
    "typecheck": "tsc --noEmit",
    "security": "semgrep --config=.semgrep.yml src/",
    "security:full": "semgrep --config=auto --config=.semgrep.yml src/",
    "security:payment": "semgrep --config=.semgrep.yml src/app/api/payments/ src/app/api/checkout/ src/lib/stripe.ts",
    "security:auth": "semgrep --config=.semgrep.yml src/app/api/auth/ src/lib/auth.ts src/lib/jwt-utils.ts",
    "security:ci": "semgrep --config=.semgrep.yml --json --output=semgrep-results.json src/",
    "security:fix": "semgrep --config=.semgrep.yml --autofix src/",
    "audit": "npm audit --audit-level=high",
    "audit:fix": "npm audit fix --force",
    "pre-deploy": "npm run security && npm run audit && npm run build",
    "db:seed": "node scripts/seed-database.js",
    "db:indexes": "node scripts/add-material-indexes.js",
    "db:indexes:test": "node scripts/add-material-indexes.js test",
    "db:indexes:analyze": "node scripts/add-material-indexes.js analyze",
    "db:indexes:rollback": "node scripts/add-material-indexes.js rollback",
    "db:performance:validate": "node scripts/validate-material-performance.js"
  },
  "dependencies": {
    "@auth/mongodb-adapter": "^3.10.0",
    "@next/swc-darwin-arm64": "^15.4.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@stripe/stripe-js": "^7.8.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lru-cache": "^7.10.9",
    "@types/node": "20.11.24",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "18.2.61",
    "@types/react-dom": "18.2.19",
    "@types/socket.io": "^3.0.1",
    "@types/socket.io-client": "^1.4.36",
    "autoprefixer": "10.4.17",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "framer-motion": "^10.16.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^11.1.0",
    "lucide-react": "^0.294.0",
    "mongodb": "^6.18.0",
    "mongoose": "^8.17.1",
    "next": "^14.2.31",
    "next-auth": "^5.0.0-beta.29",
    "nodemailer": "6.9.14",
    "postcss": "8.4.35",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recharts": "^3.1.2",
    "snappy": "^7.3.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "stripe": "^18.4.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3",
    "web-vitals": "^5.1.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.0.0",
    "axe-playwright": "^2.1.0",
    "axios": "^1.11.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.4.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "puppeteer": "^24.9.0",
    "sharp": "^0.33.5",
    "undici": "^5.28.0"
  }
}
